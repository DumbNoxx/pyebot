{
  "name": "pyechan-bot",
  "version": "1.0.0",
  "description": "Bot del Discord oficial de la comunidad Programadores y Estudiantes (PyE)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "tsc && node .",
    "dev": "tsc-watch --onSuccess \"node .\"",
    "fmt": "biome format --write",
    "fmt:check": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write"
  },
  "author": "Comunidad PyE",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/node": "^22.5.4",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "seyfert": "^3.2.4"
  }
}